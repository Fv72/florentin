<p>collapse user</p>

<div class="card">
    <div class="card-header" id="headingTwo">
        <h2 class="mb-0">
            <button class="btn btn-link btn-block text-left" type="button" data-toggle="collapse"
                data-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                User
            </button>
        </h2>
    </div>

    <div id="collapseTwo" class="collapse" aria-labelledby="headingTwo" data-parent="#accordionExample">
        <div class="card-body">

            {{!-- Create --}}
            <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#modalsCreateUser"
                data-whatever="@mdo">Créer user</button>
            {{> modals/user/modalsCreateUser }}
            <!-- Table Realisation -->
            <table class="table table-sm table-dark">
                <thead>
                    <tr>
                        <th scope="col">Mail</th>
                        <th scope="col">Nom</th>
                        <th scope="col">Prénom</th>
                        <th scope="col">Admin</th>
                        <th scope="col">Actions</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each dbUser}}
                    <tr>
                        <td>{{ email }}</td>
                        <td>{{ firstname }}</td>
                        <td>{{ lastname }}</td>
                        <td>{{ isAdmin }}</td>
                        <td>
                            {{!-- Edit --}}
                            <button type="button" class="btn btn-primary" data-toggle="modal"
                                data-target="#modalEditUser{{_id}}" data-whatever="@mdo">Editer utilisateur</button>
                        </td>
                    </tr>
                    {{> modals/modalEditUser }}
                    {{/each}}
                </tbody>
            </table>
            <!-- / Table Realisation -->
        </div>
    </div>
</div>